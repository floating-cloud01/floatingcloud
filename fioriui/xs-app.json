{
  "welcomeFile": "/fioriui/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/odata/v2",
      "authenticationType": "none",
      "destination": "srv_api",
      "csrfProtection": false
    },
    {
      "source": "^/fioriui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}